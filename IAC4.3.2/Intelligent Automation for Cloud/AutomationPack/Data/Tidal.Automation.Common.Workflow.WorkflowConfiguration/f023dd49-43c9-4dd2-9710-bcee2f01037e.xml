Function Get-NextIpAddress{
    param(
		[Parameter(Mandatory=$true)]
		[ValidatePattern("\b(?:\d{1,3}\.){3}\d{1,3}\b")]
		[String]$Address
		)
    
    $a = [System.Net.IpAddress]::Parse($address)
    $z = $a.GetAddressBytes()
    if ($z[3] -eq 255){
        $z[3] = 0
        if ($z[2] -eq 255){
            $z[2] = 0
            $z[1] += 1 }
        else{ $z[2] += 1 }
        }
    else{ $z[3] += 1 }
    $c = [System.Net.IPAddress]($z)
    return $c.ToString()
}

Function Get-SubnetDetails{
	param (
		[Parameter(Mandatory=$true)]
		[ValidatePattern("\b(?:\d{1,3}\.){3}\d{1,3}\b")]
	    [String]$SubnetAddress,
		[Parameter(Mandatory=$true)]
		[ValidateRange(8,31)]
	    [Int]$SubnetMask,
		[Parameter(Mandatory=$false)]
	    [Int]$SkipReservedCount = 1
    )
	
	$bits = @(0,128,192,224,240,248,252,254)
	$octets = $SubnetAddress.Split(".")
	$activ = $SubnetMask % 8
	$actval = $bits[$activ]
	$fulloctets = [System.Math]::Truncate($SubnetMask / 8)

	$ao = [int]$octets[$fulloctets]
	$mn = 256 â€“ $actval
	$x = [System.Math]::Truncate($ao / $mn)
	$num = $x * $mn

	$bd = $num + $mn -1
	switch ($fulloctets){
	1 {  
		$fixed = $octets[0]
		$subnet = $fixed + "." + $num.ToString() + ".0.0" 
		$strmask = "255." + $actval.ToString() + ".0.0"
		$BroadcastAddress = $fixed + "." + $bd.ToString() + ".255.255"   
		break
		}
	2 {
		$fixed = $octets[0] + "." + $octets[1]
		$subnet = $fixed + "." + $num.ToString() + ".0"
		$strmask = "255.255." + $actval.ToString() + ".0"
		$BroadcastAddress = $fixed + "." + $bd.ToString() + ".255"   
		break
	}
	3    {
		$fixed = $octets[0] + "." + $octets[1] + "." + $octets[2]
		$subnet = $fixed + "." + $num.ToString()
		$strmask = "255.255.255." + $actval.ToString() 
		$BroadcastAddress = $fixed + "." + $bd.ToString() 
		break
		}
	}
	
	#Get Gateway Address
	$SubnetAddressoct = $subnet.Split(".")
	$SubnetAddressoct[3] = ([int]$SubnetAddressoct[3] + 1).ToString()
	$fip = $SubnetAddressoct[0]+"."+$SubnetAddressoct[1]+"."+$SubnetAddressoct[2]+"."+$SubnetAddressoct[3]

	#Get Last Usable IP
	$bdoct = $BroadcastAddress.Split(".")
	$bdoct[3] = ([int]$bdoct[3] - 1).ToString()
	$lip = $bdoct[0] + "." + $bdoct[1] + "." + $bdoct[2] + "." + $bdoct[3]
	
	$o = New-Object PSObject -Property @{
	    SubnetAddress=$SubnetAddress
	    SubnetMask=$strmask
	    GatewayAddress=$fip
	    FirstUsableIP=(Get-NextIpAddress -Address $fip)
	    LastUsableIP=$lip
	    BroadcastAddress=$BroadcastAddress
		SubnetBits=$SubnetMask
		Status="Unallocated"
		VLANInstance=[String]::Empty
		Tenant=[String]::Empty
	    ReservedAddresses=[String]::Empty
		AddressType=$AddressType
	    }
		
	#Reserve additional IPs
	if ($SkipReservedCount -gt 0){
	    $ReservedAddressList=@()
	    0..$SkipReservedCount |foreach{
			$ReservedAddressList += $o.FirstUsableIP
	        $o.FirstUsableIP = Get-NextIpAddress -Address $o.FirstUsableIP
	    }
	    $o.ReservedAddresses = ($ReservedAddressList -join ",")
	}
	return $o
}

$sub = Get-SubnetDetails -SubnetAddress $NetworkAddress -SubnetMask $SubnetMaskBits -SkipReservedCount $SkipReservedIpCount
[Xml]$x = "&lt;table /&gt;"
$i = 1
$guidPrefix = [Guid]::NewGuid().ToString()
$currentIP = $sub.FirstUsableIP
do {
	$row = $x.DocumentElement.AppendChild($x.CreateElement("row"))
	$row.AppendChild($x.CreateElement("Name")).InnerText = "{0}{1}" -f $guidPrefix,$i
	$row.AppendChild($x.CreateElement("AssignedDate")).InnerText = [String]::Empty
	$row.AppendChild($x.CreateElement("IPAddress")).InnerText = $currentIP
	$row.AppendChild($x.CreateElement("Usage")).InnerText = "Unassigned"
	$row.AppendChild($x.CreateElement("UsedByMachine")).InnerText = [String]::Empty
	$row.AppendChild($x.CreateElement("VLAN")).InnerText = $VLANInstance
	if ($currentIP -eq $sub.LastUsableIP) {	break }
	else {
		$currentIP = Get-NextIpAddress -Address $currentIP
		$i++
		}
} while ($true)

$x.OuterXml</HardcodedValue>
